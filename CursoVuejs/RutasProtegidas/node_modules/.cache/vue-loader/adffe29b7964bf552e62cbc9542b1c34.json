CAAC,CAAC,CAAC,CAAC","file":"/Applications/MAMP/htdocs/CursoVuejs/RutasProtegidas/src/views/Home.vue","sourceRoot":"","sourcesContent":["<template>\n    <div>\n    <h1 class=\"my-5\"> Formlarios con vuejs</h1>\n\n        <form @submit.prevent=\"procesarForm\">\n            <Input :tarea=\"tarea\"/>\n        </form>\n        <hr>\n            <p>\n            {{ tarea }}\n            </p>\n\n             <ListaTareas/>\n    </div>\n</template>\n\n<script>\nimport Input from '../components/Input'\nimport {mapActions} from 'vuex'\nimport ListaTareas from '../components/ListaTareas.vue';\nconst shortid = require('shortid')\n\n    export default {\n        name: \"Home\",\n        components: { Input, ListaTareas },\n        data() {\n            return {\n                tarea: {\n                    id: '',\n                    nombre: '',\n                    categorias: [],\n                    estado: '',\n                    numero: 0\n                },\n            };\n        },\n        methods: {\n            ...mapActions(['setTareas']),\n            //npm i all json\n            procesarForm(){\n                console.log(this.tarea)\n                if(this.tarea.nombre.trim() === \"\"){\n                    console.log('vacio')\n                    return\n                }\n                console.log('no vacio')\n                //generar id \n                this.tarea.id = shortid.generate()\n                console.log(this.tarea.id)\n\n                //envio \n                this.setTareas(this.tarea)\n\n                //ponerlos vacios o default\n                this.tarea = {\n                    id: '',\n                    nombre: '',\n                    categorias: [],\n                    estado: '',\n                    numero: 0\n                }\n            }\n        }\n    };\n</script>"]}]}